{
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets":  {"node": "current"}
        }
      ]
    ],
    "plugins": [
        ["@babel/plugin-transform-modules-commonjs", {"allowTopLevelThis": true}],
        "@babel/plugin-proposal-class-properties",
      ],
  }